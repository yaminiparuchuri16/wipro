  <Code>Public Shared Function SetColor(ByVal Value As Integer) As String

    SetColor = "Red"

    If Value &lt;= 10000 Then
        SetColor = "Yellow"

    ElseIf Value &gt;10000 And Value &lt;20000 Then
        SetColor = "Silver"

    End If

  End Function 

Public Shared Function SetFontWeight(ByVal Value As String) As String

    SetFontWeight = "Bold"

    If Value  = "Finance" Then
        SetFontWeight  = "Normal"

 Else If Value  = "IT" Then
        SetFontWeight  = "Italic"

    End If

  End Function 

Public Shared Function SetAlternateColor(ByVal Value As Integer) As String

    SetAlternateColor= "Red"

    If Value Mod 2 =0 Then
        SetAlternateColor= "Yellow"
    End If

  End Function 
